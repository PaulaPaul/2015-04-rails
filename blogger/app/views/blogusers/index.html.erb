<p id="notice"><%= notice %></p>

<h1>Listing Blogusers</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @blogusers.each do |bloguser| %>
      <tr>
        <td><%= bloguser.name %></td>
        <td><%= link_to 'Show', bloguser %></td>
        <td><%= link_to 'Edit', edit_bloguser_path(bloguser) %></td>
        <td><%= link_to 'Destroy', bloguser, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Bloguser', new_bloguser_path %>
